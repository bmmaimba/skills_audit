  <record id="group_skills_scm_admin" model="res.groups">
    <field name="name">Skills SCM Admin</field>
    <field name="category_id" ref="base.module_category_human_resources"/>
    <field name="implied_ids" eval="[(4, ref('group_skills_scm_manager'))]"/>
  </record>
</odoo>
